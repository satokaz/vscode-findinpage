{
    "name": "vscode-findinpage",
    "displayName": "\"Find in Page\" in Preview Editor",
    "description": "Add a \"Find in Page\" button to the preview editor.",
    "version": "0.0.4",
    "publisher": "satokaz",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "media/svg/search.svg",
    "homepage": "https://github.com/satokaz/vscode-findinpage",
    "repository": {
        "type": "git",
        "url": "https://github.com/satokaz/vscode-findinpage"
    },
    "bugs": {
        "url": "https://github.com/satokaz/vscode-findinpage/issues"
    },
    "activationEvents": [
        "onCommand:extension.findinpage"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.findinpage",
                "title": "Toggle \"Find in Page\" in Preview Editor"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "babel-preset-es2015": "^6.18.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^6.1.2",
        "gulp-uglify": "^2.0.0",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "vscode-nls": "^2.0.1"
    }
}