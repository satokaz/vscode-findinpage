{
  "name": "vscode-findinpage",
  "displayName": "Previw Tools for Preview Editor",
  "description": "Add a bit two functions (search and zoom font) to the Preview Editor",
  "version": "0.1.0",
  "publisher": "satokaz",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "media/svg/search.svg",
  "homepage": "https://github.com/satokaz/vscode-findinpage",
  "repository": {
    "type": "git",
    "url": "https://github.com/satokaz/vscode-findinpage"
  },
  "bugs": {
    "url": "https://github.com/satokaz/vscode-findinpage/issues"
  },
  "activationEvents": [
    "onCommand:extension.findinpage"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.findinpage",
        "title": "Toggle Preview Tools in Preview Editor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "babel-preset-es2015": "^6.18.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-uglify": "^2.0.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "fs-extra": "^2.0.0",
    "vscode-nls": "^2.0.2"
  }
}
